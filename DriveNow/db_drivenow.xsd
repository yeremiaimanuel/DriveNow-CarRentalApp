                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    b.id_booking,
    u.nama AS nama_pelanggan,
    m.merk_mobil,
    b.tanggal_mulai,
    b.tanggal_selesai,
    b.status_booking,
    b.durasi_hari
