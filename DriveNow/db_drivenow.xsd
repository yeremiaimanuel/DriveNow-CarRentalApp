                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    FORMAT(t.tanggal_transaksi, 'yyyy-MM') AS bulan,
    COUNT(DISTINCT b.id_booking) AS total_booking,
    COUNT(DISTINCT t.id_transaksi) AS total_transaksi,
    SUM(t.total_pembayaran) AS total_pendapatan,
    COUNT(DISTINCT ul.id_ulasan) AS total_ulasan
FROM Transaksi t
JOIN Booking b ON t.id_booking = b.id_booking
LEFT JOIN Ulasan ul ON t.id_user = ul.id_user
GROUP BY FORMAT(t.tanggal_transaksi, 'yyyy-MM')
</CommandText>
